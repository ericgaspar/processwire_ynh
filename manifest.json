{
    "name": "Processwire",
    "id": "processwire",
    "packaging_format": 1,
    "description": {
        "en": "Friendly and powerful open source CMS with a strong API.",
        "fr": "Friendly and powerful open source CMS with a strong API."
    },
    "version": "3.0.200~ynh1",
    "url": "https://processwire.com/",
    "upstream": {
        "license": "MPL",
        "website": "https://processwire.com/",
        "demo": "https://processwire.com/about/demo/",
        "admindoc": "https://yunohost.org/packaging_apps",
        "code": "https://github.com/processwire/processwire"
    },
    "license": "MPL",
    "maintainer": {
        "name": "John doe",
        "email": "john.doe@example.com"
    },
    "requirements": {
        "yunohost": ">= 11.0.9"
    },
    "multi_instance": true,
    "services": [
        "nginx",
        "php7.4-fpm",
        "mysql"
    ],
    "arguments": {
        "install": [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/example",
                "default": "/example"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
